    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/libraries/{$name}/dist": [
                "type:drupal-library-fonticonpicker"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json",
                "web/modules/contrib/vuejs/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/auto_entitylabel": {
                "3065687: Integration with Layout Builder for custom block types": "https://www.drupal.org/files/issues/2024-08-12/3065687-reroll-17-auto_entitylabel-layout_builder_component_title.patch"
            },
            "drupal/google_tag": {
                "3488123: Processing commands config ID missing": "https://git.drupalcode.org/project/google_tag/-/merge_requests/96.diff"
            },
            "drupal/inline_entity_form": {
                "3033241: Cannot update the order weight using complex form widget": "https://www.drupal.org/files/issues/2021-02-15/cannot-update-weight-order-complex-widget-3033241-4.patch"
            },
            "drupal/layout_builder_browser": {
                "Filter block is not working [3456887]": "https://www.drupal.org/files/issues/2024-07-03/layout_builder_browser-search-filter-fix-2.patch",
                "Filter by block name does not hide elements with layout-builder-browser-block-item [3485031]": "https://www.drupal.org/files/issues/2024-11-01/layout_builder_browser-search-filter-fix-empty-item.patch"
            },
            "drupal/field_image_style": {
                "Drupal 11 compatibility fixes": "https://www.drupal.org/files/issues/2025-05-23/Drupal11_compatibility_fixes.patch"
            },
            "drupal/core": {
                "3330100: Cannot access offset of type string on string": "https://www.drupal.org/files/issues/2023-09-08/3330100-12.views-exposed-form-typeerror.patch"
            },
            "drupal/sitemap": {
                "3568217 Drupal 11 installation sitemap error": "https://git.drupalcode.org/project/sitemap/-/merge_requests/59.diff"
            }
        },
        "installer-types": [
            "drupal-recipe"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/core-project-message": true
        }
    }
